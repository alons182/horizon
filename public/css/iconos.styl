@font-face {
	font-family: 'rentame';
	src:url('../fonts/rentame.eot');
	src:url('../fonts/rentame.eot?#iefix') format('embedded-opentype'),
		url('../fonts/rentame.woff') format('woff'),
		url('../fonts/rentame.ttf') format('truetype'),
		url('../fonts/rentame.svg#rentame') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'rentame';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

